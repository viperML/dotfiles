{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"final_space": true,
	"osc99": true,
	"console_title": true,
	"console_title_style": "template",
	"console_title_template": "{{ .Shell }}",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"horizontal_offset": 0,
			"vertical_offset": 0,
			"segments": [
				{
					"type": "os",
					"style": "diamond",
					"foreground": "#FFF",
					"background": "#181818",
					"leading_diamond": "\uE0B6",
					"trailing_diamond": "\ue0bc",
					"properties": {
						"windows": "\ue7a2",
						"prefix": " ",
						"wsl": "",
						"wsl_separator": ""
					}
				},
				{
					"type": "text",
					"style": "powerline",
					"powerline_symbol": "\ue0ba",
					"invert_powerline": true,
					"background": "#181818",
					"foreground": "#ffffff",
					"properties": {
						"text": "",
						"prefix": "",
						"postfix": ""
					}
				},
				{
					"type": "path",
					"style": "powerline",
					"powerline_symbol": "\ue0ba",
					"invert_powerline": true,
					"foreground": "#100e23",
					"background": "#60bde9",
					"properties": {
						"style": "agnoster_full",
						"home_icon": "\uf7db",
						"folder_separator_icon": "/",
						"mapped_locations": [
							[
								"C:\\Users\\ayats\\Documents\\aurea_test1\\",
								"\uf1d8/"
							]
						]
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "\ue0ba",
					"invert_powerline": true,
					"foreground": "#193549",
					"background": "#65dd75"
				},
				{
					"type": "python",
					"style": "powerline",
					"powerline_symbol": "\ue0ba",
					"invert_powerline": true,
					"foreground": "#100e23",
					"background": "#4b49b1",
					"properties": {
						"prefix": " \uE235 ",
						"postfix": " ",
						"display_mode": "environment",
						"display_version": false
					}
				},
				{
					"type": "exit",
					"style": "powerline",
					"powerline_symbol": "\ue0ba",
					"invert_powerline": true,
					"foreground": "#a52626",
					"background": "#ffffff",
					"properties": {
						"prefix": " îˆ"
					}
				},
				{
					"type": "text",
					"style": "powerline",
					"powerline_symbol": "\ue0bc",
					"invert_powerline": false,
					"background": "#181818",
					"foreground": "#ffffff",
					"properties": {
						"text": "",
						"prefix": "",
						"postfix": ""
					}
				},
				{
					"type": "text",
					"style": "diamond",
					"trailing_diamond": "\ue0b4",
					"leading_diamond": "\ue0ba",
					"background": "#181818",
					"foreground": "#ffffff",
					"properties": {
						"text": " ",
						"prefix": "",
						"postfix": ""
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "right",
			"horizontal_offset": 0,
			"vertical_offset": 0,
			"segments": [
				{
					"type": "executiontime",
					"style": "plain",
					"foreground": "#7e7e7e",
					"properties": {
						"threshold": 0,
						"style": "amarillo"
					}
				}
			]
		},
		{
			"type": "newline"
		},
		{
			"type": "prompt",
			"alignment": "left",
			"horizontal_offset": 0,
			"vertical_offset": 0,
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"leading_diamond": ">",
					"trailing_diamond": "",
					"foreground": "#0F0",
					"properties": {
						"text": ">",
						"prefix": "  ",
						"postfix": ""
					}
				}
			]
		}
	]
}